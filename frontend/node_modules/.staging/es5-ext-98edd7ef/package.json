{
  "name": "es5-ext",
  "version": "0.10.47",
  "description": "ECMAScript extensions and shims",
  "author": "Mariusz Nowak <medyk@medikoo.com> (http://www.medikoo.com/)",
  "keywords": [
    "ecmascript",
    "ecmascript5",
    "ecmascript6",
    "es5",
    "es6",
    "extensions",
    "ext",
    "addons",
    "extras",
    "harmony",
    "javascript",
    "polyfill",
    "shim",
    "util",
    "utils",
    "utilities"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/medikoo/es5-ext.git"
  },
  "dependencies": {
    "es6-iterator": "~2.0.3",
    "es6-symbol": "~3.1.1",
    "next-tick": "1"
  },
  "devDependencies": {
    "eslint": "^5.12",
    "eslint-config-medikoo-es5": "2",
    "plain-promise": "^0.1.1",
    "tad": "^0.2.8"
  },
  "eslintConfig": {
    "extends": "medikoo-es5",
    "root": true,
    "rules": {
      "no-extend-native": "off"
    },
    "overrides": [
      {
        "files": [
          "array/from/shim.js",
          "test/**"
        ],
        "rules": {
          "max-lines": "off"
        }
      }
    ]
  },
  "scripts": {
    "lint": "eslint --ignore-path=.gitignore .",
    "test": "node ./node_modules/tad/bin/tad"
  },
